<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wjd.jetpacklearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackLearn">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewbinding.ViewBindingActivity" />
        <activity android:name="com.wjd.jetpacklearn.viewmodel.simple.ViewModelActivity" />
        <activity android:name=".viewmodel.multi_fragment.MultiFragmentActivity" />
        <activity android:name=".livedata.timer.TimerLiveDataActivity" />
        <activity android:name=".livedata.multi_fragment.MultiFragmentActivity" />
        <activity
            android:name=".room.RoomActivity"
            android:windowSoftInputMode="adjustPan|stateAlwaysHidden" />
    </application>

</manifest>